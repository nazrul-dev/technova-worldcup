// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Participant {
  id           Int      @id @default(autoincrement())
  name         String
  email        String   @unique
  organization String?
  phone        String?
  prediction   String
  createdAt    DateTime @default(now())
}

model Teams {
  id    Int     @id @default(autoincrement())
  code  String?
  value String? @unique
  label String? @unique
  flag  String?
}
